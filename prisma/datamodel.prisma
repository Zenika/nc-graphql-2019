type Plaine {
  id: ID! @id
  createdAt: DateTime! @createdAt
  mares: [Mare]
}

type Mare {
  id: ID! @id
  createdAt: DateTime! @createdAt
  nom: String
  plaine: Plaine!
  canards: [Canard]
  poissons: [Poisson]
}

type Canard {
  id: ID! @id
  createdAt: DateTime! @createdAt
  mare: Mare!
  nom: String
  isAffame: Boolean
}

type Poisson {
  id: ID! @id
  createdAt: DateTime! @createdAt
  mare: Mare!
  isGros: Boolean
  isCanardvore: Boolean
}
